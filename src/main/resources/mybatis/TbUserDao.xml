<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yiyi.yibaifen.springboottest.dao.TbUserDao">
    <insert id="insert" parameterType="com.yiyi.yibaifen.springboottest.model.TbUser">
        insert into tb_user(username,mobile,password,create_time)
        values(#{userName},#{mobile},#{password},now());
    </insert>
</mapper>